 					//{
 					//	"AxisName":"AXIS_BRAKE",
 					//	"SrcCode": 23,
 					//	"DesCode": 55
 					//}
 				]
      
      },
      "InputUsage": {
        "LongPressMS": 3000,
        "RepeatSentKeyEvent": false,
        "RepeatInterval": 15
      }
